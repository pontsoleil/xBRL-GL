<schema targetNamespace="http://www.xbrl.org/int/gl/ehm/2025-12-01" elementFormDefault="qualified" attributeFormDefault="unqualified"
  xmlns="http://www.w3.org/2001/XMLSchema"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:xbrli="http://www.xbrl.org/2003/instance"
  xmlns:gl-ehm="http://www.xbrl.org/int/gl/ehm/2025-12-01">
  <import namespace="http://www.xbrl.org/2003/instance" schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd"/>
  <include schemaLocation="../ehm/gl-ehm-2025-12-01.xsd"/>
  <!-- tuple data type -->
  <group name="serialOrLotGroup">
    <sequence>
      <element ref="gl-ehm:serialOrLotCode" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotDesc" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotNum" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotNextNum" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotQuantity" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotOriginalQuantity" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotRemainingQuantity" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotOrigination" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotExpiration" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotManufacturer" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotBatchDesc" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotWarrantyStartDate" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotWarrantyEndDate" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotWarrantyPeriod" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotWarrantPeriodUnit" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotWarrantyVendor" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotWarrantyContract" minOccurs="0"/>
      <element ref="gl-ehm:serialOrLotComment" minOccurs="0"/>
    </sequence>
  </group>
  <complexType name="serialOrLotComplexType">
    <complexContent>
      <restriction base="anyType">
        <sequence>
          <group ref="gl-ehm:serialOrLotGroup"/>
        </sequence>
        <attribute name="id" type="ID"/>
      </restriction>
    </complexContent>
  </complexType>
</schema>