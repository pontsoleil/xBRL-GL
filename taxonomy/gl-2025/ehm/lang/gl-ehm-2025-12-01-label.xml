<linkbase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xbrl.org/2003/linkbase http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd"
    xmlns="http://www.xbrl.org/2003/linkbase"
    xmlns:xlink="http://www.w3.org/1999/xlink">
    <labelLink xlink:type="extended" xlink:role="http://www.xbrl.org/2003/role/link">
        <!-- gl-ehm:serialOrLot Serial or Lot -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLot" xlink:label="serialOrLot"/>
        <label xlink:type="resource" xlink:label="serialOrLot_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLot_en" xml:lang="en">Serial or Lot</label>
        <label xlink:type="resource" xlink:label="serialOrLot_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Tuple for holding information about serial numbers or lots (batches). As a tuple, this permits multiple serial numbers or multiple lot number batches within a single gl-cor:measurable structure.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLot" xlink:to="serialOrLot_lbl"/>
        <!-- gl-ehm:serialOrLotCode Code -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotCode" xlink:label="serialOrLotCode"/>
        <label xlink:type="resource" xlink:label="serialOrLotCode_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotCode_en" xml:lang="en">Code</label>
        <label xlink:type="resource" xlink:label="serialOrLotCode_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Is this item tracked by serial numbers or lot numbers? A serial number is usually a unique number assigned to a single; a lot or batch number is provided when a batch of items is fungible but must be differentiated from similar items from other batches. Enumerated indicator for serial or lot items. Enumeration are: serial, lot, other, neither. If not included within the tuple, neither is assumed.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotCode" xlink:to="serialOrLotCode_lbl"/>
        <!-- gl-ehm:serialOrLotDesc Description -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotDesc" xlink:label="serialOrLotDesc"/>
        <label xlink:type="resource" xlink:label="serialOrLotDesc_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotDesc_en" xml:lang="en">Description</label>
        <label xlink:type="resource" xlink:label="serialOrLotDesc_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Free format description associated with gl-ehm:serialLotCode to provide specialization and especially to provide clarification for the enumerated value of other in serialLotCode. Examples might be “batch” or “group”.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotDesc" xlink:to="serialOrLotDesc_lbl"/>
        <!-- gl-ehm:serialOrLotNum Number -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotNum" xlink:label="serialOrLotNum"/>
        <label xlink:type="resource" xlink:label="serialOrLotNum_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotNum_en" xml:lang="en">Number</label>
        <label xlink:type="resource" xlink:label="serialOrLotNum_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">The holder for that actual serial number or lot (batch) number/code assigned to an item or a batch of items.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotNum" xlink:to="serialOrLotNum_lbl"/>
        <!-- gl-ehm:serialOrLotNextNum Next Number -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotNextNum" xlink:label="serialOrLotNextNum"/>
        <label xlink:type="resource" xlink:label="serialOrLotNextNum_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotNextNum_en" xml:lang="en">Next Number</label>
        <label xlink:type="resource" xlink:label="serialOrLotNextNum_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">A field for holding the next expected serial or lot number to be given to future assignments, or for chaining between already assigned items.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotNextNum" xlink:to="serialOrLotNextNum_lbl"/>
        <!-- gl-ehm:serialOrLotQuantity Quantity -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotQuantity" xlink:label="serialOrLotQuantity"/>
        <label xlink:type="resource" xlink:label="serialOrLotQuantity_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotQuantity_en" xml:lang="en">Quantity</label>
        <label xlink:type="resource" xlink:label="serialOrLotQuantity_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">The quantity of units in this particular batch. Unit of measure is the gl-bus:measurableUnitofMeasure.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotQuantity" xlink:to="serialOrLotQuantity_lbl"/>
        <!-- gl-ehm:serialOrLotOriginalQuantity Original Quantity -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotOriginalQuantity" xlink:label="serialOrLotOriginalQuantity"/>
        <label xlink:type="resource" xlink:label="serialOrLotOriginalQuantity_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotOriginalQuantity_en" xml:lang="en">Original Quantity</label>
        <label xlink:type="resource" xlink:label="serialOrLotOriginalQuantity_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">The quantity of units in this batch on inception.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotOriginalQuantity" xlink:to="serialOrLotOriginalQuantity_lbl"/>
        <!-- gl-ehm:serialOrLotRemainingQuantity Remaining Quantity -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotRemainingQuantity" xlink:label="serialOrLotRemainingQuantity"/>
        <label xlink:type="resource" xlink:label="serialOrLotRemainingQuantity_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotRemainingQuantity_en" xml:lang="en">Remaining Quantity</label>
        <label xlink:type="resource" xlink:label="serialOrLotRemainingQuantity_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">The remaining quantity of units from the original batch; may be the same as the quantity in this batch or include other batches from the same original batch.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotRemainingQuantity" xlink:to="serialOrLotRemainingQuantity_lbl"/>
        <!-- gl-ehm:serialOrLotOrigination Origination -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotOrigination" xlink:label="serialOrLotOrigination"/>
        <label xlink:type="resource" xlink:label="serialOrLotOrigination_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotOrigination_en" xml:lang="en">Origination</label>
        <label xlink:type="resource" xlink:label="serialOrLotOrigination_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Date of manufacture or creation.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotOrigination" xlink:to="serialOrLotOrigination_lbl"/>
        <!-- gl-ehm:serialOrLotExpiration Expiration -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotExpiration" xlink:label="serialOrLotExpiration"/>
        <label xlink:type="resource" xlink:label="serialOrLotExpiration_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotExpiration_en" xml:lang="en">Expiration</label>
        <label xlink:type="resource" xlink:label="serialOrLotExpiration_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Expiration date of the batch.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotExpiration" xlink:to="serialOrLotExpiration_lbl"/>
        <!-- gl-ehm:serialOrLotManufacturer Manufacturer -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotManufacturer" xlink:label="serialOrLotManufacturer"/>
        <label xlink:type="resource" xlink:label="serialOrLotManufacturer_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotManufacturer_en" xml:lang="en">Manufacturer</label>
        <label xlink:type="resource" xlink:label="serialOrLotManufacturer_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Manufacturer of the item (serial) or items (lot/batch).</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotManufacturer" xlink:to="serialOrLotManufacturer_lbl"/>
        <!-- gl-ehm:serialOrLotBatchDesc Batch Description -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotBatchDesc" xlink:label="serialOrLotBatchDesc"/>
        <label xlink:type="resource" xlink:label="serialOrLotBatchDesc_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotBatchDesc_en" xml:lang="en">Batch Description</label>
        <label xlink:type="resource" xlink:label="serialOrLotBatchDesc_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">A name or description of the items in the lot or batch.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotBatchDesc" xlink:to="serialOrLotBatchDesc_lbl"/>
        <!-- gl-ehm:serialOrLotWarrantyStartDate Warranty Start Date -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotWarrantyStartDate" xlink:label="serialOrLotWarrantyStartDate"/>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantyStartDate_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotWarrantyStartDate_en" xml:lang="en">Warranty Start Date</label>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantyStartDate_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">If the items are covered by warranty, when that warranty covered will begin or has begun.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotWarrantyStartDate" xlink:to="serialOrLotWarrantyStartDate_lbl"/>
        <!-- gl-ehm:serialOrLotWarrantyEndDate Warranty End Date -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotWarrantyEndDate" xlink:label="serialOrLotWarrantyEndDate"/>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantyEndDate_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotWarrantyEndDate_en" xml:lang="en">Warranty End Date</label>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantyEndDate_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">If the items are covered by warranty, when that warranty covered will end or ended.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotWarrantyEndDate" xlink:to="serialOrLotWarrantyEndDate_lbl"/>
        <!-- gl-ehm:serialOrLotWarrantyPeriod Warranty Period -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotWarrantyPeriod" xlink:label="serialOrLotWarrantyPeriod"/>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantyPeriod_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotWarrantyPeriod_en" xml:lang="en">Warranty Period</label>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantyPeriod_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Warranty period – numeric portion; for a 30 day warranty period, the 30 would go here, and the day would go in gl-ehm:serialLotWarrantyPeriodUnit.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotWarrantyPeriod" xlink:to="serialOrLotWarrantyPeriod_lbl"/>
        <!-- gl-ehm:serialOrLotWarrantPeriodUnit Warrant Period Unit -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotWarrantPeriodUnit" xlink:label="serialOrLotWarrantPeriodUnit"/>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantPeriodUnit_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotWarrantPeriodUnit_en" xml:lang="en">Warrant Period Unit</label>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantPeriodUnit_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Warrant period – time or cycle unit portion. Values from the XBRL Unit Registry (www.xbrl.org/utr/utr.xml) are suggested where possible. Examples might include: H (hours), D (days), M (months), Y (years), machine cycles, km (kilometers).</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotWarrantPeriodUnit" xlink:to="serialOrLotWarrantPeriodUnit_lbl"/>
        <!-- gl-ehm:serialOrLotWarrantyVendor Warranty Vendor -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotWarrantyVendor" xlink:label="serialOrLotWarrantyVendor"/>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantyVendor_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotWarrantyVendor_en" xml:lang="en">Warranty Vendor</label>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantyVendor_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">The vendor providing warranty; this may differ from the manufacturer.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotWarrantyVendor" xlink:to="serialOrLotWarrantyVendor_lbl"/>
        <!-- gl-ehm:serialOrLotWarrantyContract Warranty Contract -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotWarrantyContract" xlink:label="serialOrLotWarrantyContract"/>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantyContract_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotWarrantyContract_en" xml:lang="en">Warranty Contract</label>
        <label xlink:type="resource" xlink:label="serialOrLotWarrantyContract_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Contract or document information related to the warranty, such as a contract number and date or a URL with more information.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotWarrantyContract" xlink:to="serialOrLotWarrantyContract_lbl"/>
        <!-- gl-ehm:serialOrLotComment Comment -->
        <loc xlink:type="locator" xlink:href="../gl-ehm-2025-12-01.xsd#gl-ehm_serialOrLotComment" xlink:label="serialOrLotComment"/>
        <label xlink:type="resource" xlink:label="serialOrLotComment_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-ehm_serialOrLotComment_en" xml:lang="en">Comment</label>
        <label xlink:type="resource" xlink:label="serialOrLotComment_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Free format text related to the serial number or lot.</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="serialOrLotComment" xlink:to="serialOrLotComment_lbl"/>
  </labelLink>
</linkbase>
