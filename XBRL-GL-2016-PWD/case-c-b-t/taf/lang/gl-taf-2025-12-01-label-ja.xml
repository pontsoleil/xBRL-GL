    xmlns="http://www.xbrl.org/2003/linkbase"
    xmlns:xlink="http://www.w3.org/1999/xlink">
    <labelLink xlink:type="extended" xlink:role="http://www.xbrl.org/2003/role/link">
        <!-- gl-taf:tickingField チェック欄 -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2025-12-01.xsd#gl-taf_tickingField" xlink:label="tickingField"/>
        <label xlink:type="resource" xlink:label="tickingField_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_tickingField_ja" xml:lang="en">チェック欄</label>
        <label xlink:type="resource" xlink:label="tickingField_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">項目が消去または完了されたことを示す。勘定のチェックが要求される場合、処理済を示すチェックマークまたは文字が指定される。支払またはDR/CRのメモに対する元文書が、消し込まれたことを確認するためにも使用可能。</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="tickingField" xlink:to="tickingField_lbl"/>
        <!-- gl-taf:documentRemainingBalance 文書上残高 -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2025-12-01.xsd#gl-taf_documentRemainingBalance" xlink:label="documentRemainingBalance"/>
        <label xlink:type="resource" xlink:label="documentRemainingBalance_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_documentRemainingBalance_ja" xml:lang="en">文書上残高</label>
        <label xlink:type="resource" xlink:label="documentRemainingBalance_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">文書上の残高。</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="documentRemainingBalance" xlink:to="documentRemainingBalance_lbl"/>
        <!-- gl-taf:uniqueConsignmentReference UCRコード -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2025-12-01.xsd#gl-taf_uniqueConsignmentReference" xlink:label="uniqueConsignmentReference"/>
        <label xlink:type="resource" xlink:label="uniqueConsignmentReference_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_uniqueConsignmentReference_ja" xml:lang="en">UCRコード</label>
        <label xlink:type="resource" xlink:label="uniqueConsignmentReference_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">Unique Consignment ReferenceまたはUCR。国際委託販売のための"origin to destination"参照コード。World Customs OrganizationとEAN International (EAN)が共同で開発している。
(http://www.wcoomd.org/ie/EN/press/UCR_new_e.pdf)</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="uniqueConsignmentReference" xlink:to="uniqueConsignmentReference_lbl"/>
        <!-- gl-taf:originatingDocumentStructure 【発生元文書情報】 -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2025-12-01.xsd#gl-taf_originatingDocumentStructure" xlink:label="originatingDocumentStructure"/>
        <label xlink:type="resource" xlink:label="originatingDocumentStructure_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_originatingDocumentStructure_ja" xml:lang="en">【発生元文書情報】</label>
        <label xlink:type="resource" xlink:label="originatingDocumentStructure_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">業務プロセス上の、関連文書の流れを捕捉するためのもの。特にVAT監査では、エンドユーザの要求として、取引に関係する文書の履歴が必要とされる。関連文書情報の構造の繰り返しを含む。</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="originatingDocumentStructure" xlink:to="originatingDocumentStructure_lbl"/>
        <!-- gl-taf:originatingDocumentType 発生元文書種別 -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2025-12-01.xsd#gl-taf_originatingDocumentType" xlink:label="originatingDocumentType"/>
        <label xlink:type="resource" xlink:label="originatingDocumentType_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_originatingDocumentType_ja" xml:lang="en">発生元文書種別</label>
        <label xlink:type="resource" xlink:label="originatingDocumentType_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">発生元文書の種別。小切手(check)、借方メモ(debit-memo)、貸方メモ(credit-memo)、財務費用(finance-charge)、顧客への請求書(invoice)、顧客オーダ(order-customer)、仕入先オーダ(order-vendor)、その他支払い(payment-other)、督促状(reminder)、手形(tegata)、ベンダからの請求書(voucher)、出荷票(shipment)、受領票(receipt)、手修正(manual-adjustment)、その他(other)</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="originatingDocumentType" xlink:to="originatingDocumentType_lbl"/>
        <!-- gl-taf:originatingDocumentNumber 発生元文書番号 -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2025-12-01.xsd#gl-taf_originatingDocumentNumber" xlink:label="originatingDocumentNumber"/>
        <label xlink:type="resource" xlink:label="originatingDocumentNumber_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_originatingDocumentNumber_ja" xml:lang="en">発生元文書番号</label>
        <label xlink:type="resource" xlink:label="originatingDocumentNumber_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">発生元文書の番号。</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="originatingDocumentNumber" xlink:to="originatingDocumentNumber_lbl"/>
        <!-- gl-taf:originatingDocumentDate 発生元文書日付 -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2025-12-01.xsd#gl-taf_originatingDocumentDate" xlink:label="originatingDocumentDate"/>
        <label xlink:type="resource" xlink:label="originatingDocumentDate_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_originatingDocumentDate_ja" xml:lang="en">発生元文書日付</label>
        <label xlink:type="resource" xlink:label="originatingDocumentDate_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">発生元文書の日付。</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="originatingDocumentDate" xlink:to="originatingDocumentDate_lbl"/>
        <!-- gl-taf:originatingDocumentIdentifierType 発生元文書対象CVE種別 -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2025-12-01.xsd#gl-taf_originatingDocumentIdentifierType" xlink:label="originatingDocumentIdentifierType"/>
        <label xlink:type="resource" xlink:label="originatingDocumentIdentifierType_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_originatingDocumentIdentifierType_ja" xml:lang="en">発生元文書対象CVE種別</label>
        <label xlink:type="resource" xlink:label="originatingDocumentIdentifierType_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">発生元文書の対象となるCVEの種別。 [設定値のリスト] (C)、(customer：顧客)、(E)、(employee：従業員)、(V)、(vendor：仕入先)、(O)、(other：その他)、(I)、(salesperson-internal：店舗内販売員)、(X)、(salesperson-external：外交販売員)、(N)、(contractor：請負者)</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="originatingDocumentIdentifierType" xlink:to="originatingDocumentIdentifierType_lbl"/>
        <!-- gl-taf:originatingDocumentIdentifierCode 発生元文書対象CVEコード -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2025-12-01.xsd#gl-taf_originatingDocumentIdentifierCode" xlink:label="originatingDocumentIdentifierCode"/>
        <label xlink:type="resource" xlink:label="originatingDocumentIdentifierCode_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_originatingDocumentIdentifierCode_ja" xml:lang="en">発生元文書対象CVEコード</label>
        <label xlink:type="resource" xlink:label="originatingDocumentIdentifierCode_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">発生元文書の対象となるCVEのコード。</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="originatingDocumentIdentifierCode" xlink:to="originatingDocumentIdentifierCode_lbl"/>
        <!-- gl-taf:originatingDocumentIdentifierTaxCode 発生元文書対象CVE納税者コード -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2025-12-01.xsd#gl-taf_originatingDocumentIdentifierTaxCode" xlink:label="originatingDocumentIdentifierTaxCode"/>
        <label xlink:type="resource" xlink:label="originatingDocumentIdentifierTaxCode_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_originatingDocumentIdentifierTaxCode_ja" xml:lang="en">発生元文書対象CVE納税者コード</label>
        <label xlink:type="resource" xlink:label="originatingDocumentIdentifierTaxCode_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">発生元文書の対象となるCVEの納税者コード。</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="originatingDocumentIdentifierTaxCode" xlink:to="originatingDocumentIdentifierTaxCode_lbl"/>
