  <complexType name="summaryReportingTaxonomyComplexType">
    <complexContent>
      <restriction base="anyType">
        <sequence>
          <group ref="gl-srcd:summaryReportingTaxonomyGroup"/>
        </sequence>
        <attribute name="id" type="ID"/>
      </restriction>
    </complexContent>
